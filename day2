#include <iostream>
using namespace std;

int missingval(int arr[], int N) 
{
    int xor1 = 0, xor2 = 0;
    int n=N-1;
    for(int i = 0; i < n; i++) 
    {
        xor2 ^= arr[i];
        xor1 ^=(i+1);
    }
    xor1=xor1^N;
    return xor1 ^ xor2;
}

int main() 
{
    int N;
    cin >> N;   
    int a[N - 1];   

    for (int i = 0; i < N - 1; i++) 
    {
        cin >> a[i];
    }

    cout << missingval(a, N);
    return 0;
}
