#include <iostream>
#include <vector>
using namespace std;

void display(vector<int>& arr) {
    cout << endl;
    for (int x : arr) {
        cout << x << " ";
    }
    cout << endl;
}


void mergee(vector<int>& A, vector<int>& B, int m, int n) 
{
    int i = m - 1;        
    int j = n - 1;        
    int k = m + n - 1;    

    while (i >= 0 && j >= 0) 
    {
        if (A[i] > B[j]) 
        {
            A[k] = A[i];
            i--;
        } else {
            A[k] = B[j];
            j--;
        }
        k--;
    }

    while (j >= 0) 
    {
        A[k] = B[j];
        j--;
        k--;
    }

    display(A);
}

int main() 
{
    int m, n;
    cout << "Enter number of valid elements in A (m): ";
    cin >> m;

    cout << "Enter number of elements in B (n): ";
    cin >> n;

    vector<int> A(m + n); 
    vector<int> B(n);

    cout << "Enter " << m << " sorted elements of A:" << endl;
    for (int i = 0; i < m; i++) {
        cin >> A[i];
    }

    cout << "Enter " << n << " sorted elements of B:" << endl;
    for (int i = 0; i < n; i++) {
        cin >> B[i];
    }

    display(A);
    display(B);

    mergee(A, B, m, n);

    return 0;
}
