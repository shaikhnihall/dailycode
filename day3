#include <iostream>
using namespace std;

int removedup(int arr[], int n) 
{
    int ptr1 = arr[0];
    int ptr2 = arr[0];

    do 
    {
        ptr1 = arr[ptr1];         
        ptr2 = arr[arr[ptr2]];     
    } while (ptr1 != ptr2);

   
    ptr1 = arr[0];
    while (ptr1 != ptr2) 
    {
        ptr1 = arr[ptr1];
        ptr2 = arr[ptr2];
    }

    return ptr1; 
}

int main() 
{
    int n;
    cin >> n;  
    int arr[n+1];
    for (int i = 0; i < n+1; i++) 
    {
        cin >> arr[i];
    }

    cout << "Duplicate number: " << removedup(arr, n) << endl;
    return 0;
}
